<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="backend.fxcontrollers.mainViewController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="startServerButton" layoutX="115.0" layoutY="231.0" mnemonicParsing="false" onAction="#startServer" text="Run Server" />
            <Button fx:id="startClientButton" layoutX="394.0" layoutY="231.0" mnemonicParsing="false" text="Run Client" />
            <ImageView fx:id="startServerImage" fitHeight="150.0" fitWidth="200.0" layoutX="71.0" layoutY="44.0" pickOnBounds="true" preserveRatio="true" />
            <ImageView fx:id="startClientImage" fitHeight="150.0" fitWidth="200.0" layoutX="356.0" layoutY="44.0" pickOnBounds="true" preserveRatio="true" />
         </children>
    </AnchorPane>
  </children>
</VBox>
